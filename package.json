{
  "name": "templating",
  "version": "1.0.0",
  "description": "evan's templates for creating new projects",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "author": "evan kirkiles",
  "license": "MIT",
  "type": "module",
  "files": [
    "dist/**/*"
  ],
  "scripts": {
    "prepare": "npm run build && husky install",
    "prepublish": "bash .git/hooks/pre-commit",
    "preversion": "bash .git/hooks/pre-commit",
    "version": "npm run format && git add -A templates && git add index.ts",
    "postversion": "git push && git push --tags",
    "start": "node dist/index.js",
    "build": "tsc"
  },
  "bin": {
    "evans-templates": "dist/index.js"
  },
  "dependencies": {
    "inquirer": "^9.1.4"
  },
  "devDependencies": {
    "@types/inquirer": "^9.0.2",
    "@types/node": "^18.11.3",
    "eslint": "8.22.0",
    "eslint-config-next": "12.3.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-prettier": "^4.2.1",
    "husky": ">=6",
    "lint-staged": ">=10",
    "prettier": "^2.7.1",
    "tslint-config-prettier": "^1.18.0",
    "typescript": "4.8.3"
  },
  "lint-staged": {
    "*.{ts,tsx}": [
      "prettier --write",
      "eslint --fix"
    ],
    "*.scss": [
      "prettier --write"
    ]
  }
}
